#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>

typedef struct node {
    int data;
    int color;
    int d;
    int pi;
    struct node *next;
} node;

node* createnode(int item) {
    node *temp = malloc(sizeof(node));
    if (temp == NULL) {
        fprintf(stderr, "Error: unable to allocate memory\n");
        exit(1);
    }
    temp->next = NULL;
    temp->data = item;
    temp->color = 0;
    temp->d = 999;
    temp->pi = 0;
    return temp;
}

node* insert_at_end(node* start, int item) {
    node *temp, *ptr;
    temp = createnode(item);
    if (start == NULL) {
        start = temp;
    } else {
        ptr = start;
        while (ptr->next != NULL)
            ptr = ptr->next;
        ptr->next = temp;
    }
    return start;
}

void disp_adjlist(node** adj, int v) {
    node* temp;
    int i;
    for (i = 0; i < v; i++) {
        printf("V%d: ", i);
        temp = adj[i];
        while (temp != NULL) {
            printf("%d %d %d %d\t", temp->data, temp->color, temp->d, temp->pi);
            temp = temp->next;
        }
        printf("\n");
    }
}

